{
	"_version": "1.7.0",
	"sap.app": {
		"id": "UI5OCRTickets",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {
			"PatternsRegEx": {
				"uri": "model/regExPatterns.json",
				"type": "JSON"
			}
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"

		]
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "UI5OCRTickets.view.Main",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewPath": "UI5OCRTickets.view",
				"controlId": "app",
				"controlAggregation": "pages"
			},
			"routes": [
				{
					"name": "Main",
					"pattern": "",
					"target": "Main"

				},
				{
					"name": "Products",
					"pattern": "Main",
					"target": 	"Products"

				},
				{
					"name": "InputProduct",
					"pattern": "Products",
					"target": 	"InputProduct"

				}
			],
			"targets": {
				"Main": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "Main"

				},

				"Products": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "Products"
				},
				"InputProduct": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "InputProduct"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "UI5OCRTickets.i18n.i18n"
				}
			},
			"oRegExModel": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "PatternsRegEx"
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		}
	}
}